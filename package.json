		"lodash": "4.17.21",
		"prop-types": "15.8.1"
	},
	"devDependencies": {
		"@emotion/babel-plugin-jsx-pragmatic": "0.2.0",
		"@emotion/babel-preset-css-prop": "11.10.0",
		"@emotion/react": "11.10.0",
		"@wordpress/scripts": "23.7.2",
		"husky": "8.0.1",
		"lint-staged": "13.0.3",
		"prettier": "npm:wp-prettier@^2.6.2"
	}
}
